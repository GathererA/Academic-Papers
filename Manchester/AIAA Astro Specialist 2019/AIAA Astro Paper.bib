@article{Dalle,
  title = {Characterization of {{Entry}} to {{Solar System Atmospheres}} for {{Chip}}-{{Sized Spacecraft}}},
  abstract = {Because chip-scale spacecraft could be deployed in large numbers at low cost, they could be used to collect data on a large portion of an atmosphere even if some fraction of the spacecraft do not succeed. When entering a planetary or lunar atmosphere, a satellite with a high ratio of surface area to mass tends to decelerate at higher altitudes and thus incur less aerodynamic heating. This is the effect of a well-known parameter called the ballistic coefficient. For extremely low values of ballistic coefficient, it may be possible to enter an atmosphere without an extensive thermal protection system. In this paper we discuss chip-scale spacecraft and simulate them entering all of the significant atmospheres of the solar system: Venus, Earth, Mars, Jupiter, Saturn, Uranus, Neptune, and Saturn's moon Titan. A chip-scale satellite could provide sought-after data for planetary atmospheres, especially the tenuous upper atmospheres that are difficult to study using radio occultation. This study introduces a low-cost architecture to obtain useful scientific data about upper atmospheres and reentry itself. Practical challenges associated with the design concept are also discussed.},
  language = {en},
  author = {Dalle, Derek J and Spangelo, Sara C},
